{
    "image": "mcr.microsoft.com/devcontainers/universal:2",
    "forwardPorts": [
        "monetr:80",
        "delve:2345"
    ],
    "portsAttributes": {
        "80": {
            "label": "monetr",
            "onAutoForward": "openBrowser",
            "elevateIfNeeded": true
        }
    },
    "features": {
	    "ghcr.io/devcontainers/features/sshd:1": {
		    "version": "latest"
	    }
    },
    "shutdownAction": "stopContainer",
    "hostRequirements": {
        "cpus": 4,
        "memory": "4gb"
    },
    "postCreateCommand": [],
    "postStartCommand": [],
    "postAttachCommand": [],
    "features": {}
}
